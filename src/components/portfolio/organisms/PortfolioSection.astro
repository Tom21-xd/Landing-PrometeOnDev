---
import PortfolioGrid from "../molecules/PortfolioGrid.astro";
import PortfolioTittle from "../atoms/PortfolioTittle.astro";

interface Props {
  title: string;
}

const { title } = Astro.props as Props;
---

<section
  class="container mx-auto pt-24 px-6 md:px-10 lg:px-12 flex flex-col gap-6"
>
  <PortfolioTittle title={title} />

  <PortfolioGrid />
</section>
