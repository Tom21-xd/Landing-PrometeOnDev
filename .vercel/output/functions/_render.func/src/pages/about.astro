---
import Layout from "../layouts/Layout.astro";
import TeamPreviewSection from "@/components/about/organisms/teamPreviewSection.astro";
import MissionSection from "@/components/about/organisms/missionSection.astro";
import HistorySection from "@/components/about/organisms/historySection.astro";
import StatsSection from "@/components/about/organisms/statsSection.astro";
import WhoWeAreSection from "@/components/about/organisms/whoWeAreSection.astro";
import ValuesCarousel from "@/components/about/organisms/valuesCarousel.astro";
import SectionTitleUp from "@/components/about/organisms/sectionTitleUp.astro";

const title = "Quiénes somos en PrometeonDev - Historia, misión y equipo";
const description =
  "Descubre quiénes somos en PrometeonDev: conoce nuestra historia, misión, valores y al equipo que impulsa soluciones digitales a tu medida.";
const keywords =
  "quiénes somos, equipo de desarrollo, historia PrometeonDev, misión empresarial, valores corporativos, empresa de software, soluciones digitales, desarrollo web Colombia, Prometeon, PrometeonDev, PrometeonDev Colombia";
---

<Layout {title} {description} {keywords}>
  <SectionTitleUp />

  <WhoWeAreSection />

  <StatsSection />

  <HistorySection />

  <ValuesCarousel />

  <MissionSection />

  <TeamPreviewSection />
</Layout>
